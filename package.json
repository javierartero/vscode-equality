{
  "name": "equality",
  "displayName": "equality",
  "description": "=calc (press ctrl + enter)",
  "version": "0.1.0",
  "publisher": "javierartero",
  "engines": {
    "vscode": "^1.12.0"
  },
  "icon": "images/icon.png",
  "galleryBanner": {
    "color": "#503F2F",
    "theme": "dark"
  },
  "homepage": "https://github.com/javierartero/equality/blob/master/README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/javierartero/equality"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.equality"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "keybindings": [
      {
        "command": "extension.equality",
        "key": "Ctrl+Enter"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.32",
    "@types/node": "^6.0.40",
    "faker": "^4.1.0",
    "mocha": "^2.3.3",
    "typescript": "^2.0.3",
    "vscode": "^1.0.0"
  }
}
